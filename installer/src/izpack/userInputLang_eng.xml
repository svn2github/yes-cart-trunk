    <str id="WebServerConfigurationPanel.ssl.keystore.file.password.validation.error"
         txt="Keystore password validation failed."/>
    <str id="WebServerConfigurationPanelValidator.validation.failed" txt="Following validation(s) failed:"/>
    <str id="WebServerConfigurationPanelValidator.http.port.unavailable" txt="HTTP port is unavailable."/>
    <str id="WebServerConfigurationPanelValidator.https.port.unavailable" txt="HTTPS port is unavailable."/>
    <str id="WebServerConfigurationPanelValidator.validation.confirmation" txt="Do you want to proceed?"/>

</langpack>
