<import-descriptor>

    <entityIntface>org.yes.cart.domain.entity.ProductTypeAttr</entityIntface>

    <importFile class="file-descriptor">
        <columnDelimeter>;</columnDelimeter>
        <textQualifier>&quot;</textQualifier>
        <ignoreFirstLine>true</ignoreFirstLine>
        <fileNameMask>producttypeattr.csv</fileNameMask>
        <fileEncoding>UTF-8</fileEncoding>
    </importFile>

    <importColumns class="list">

        <column-descriptor>
            <columnIndex>4</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>guid</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select b from ProductTypeAttr b where b.guid = ?1</lookupQuery>
        </column-descriptor>


        <column-descriptor>
            <columnIndex>0</columnIndex>
            <fieldType>FK_FIELD</fieldType>
            <name>producttype</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select b from ProductTypeEntity b where b.name = ?1</lookupQuery>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>1</columnIndex>
            <fieldType>FK_FIELD</fieldType>
            <name>attribute</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select b from AttributeEntity b where b.code = ?1</lookupQuery>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>5</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>navigation</name>
            <regExp>(.*)</regExp>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>6</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>navigationType</name>
            <regExp>(.*)</regExp>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>7</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>rangeNavigation</name>
            <regExp>(.*)</regExp>
        </column-descriptor>



    </importColumns>
    <importFolder></importFolder>
</import-descriptor>