<import-descriptor>

    <entityIntface>org.yes.cart.domain.entity.ProductType</entityIntface>

    <importFile class="file-descriptor">
        <columnDelimeter>;</columnDelimeter>
        <textQualifier>&quot;</textQualifier>
        <ignoreFirstLine>true</ignoreFirstLine>
        <fileNameMask>category.csv</fileNameMask>
        <fileEncoding>UTF-8</fileEncoding>
    </importFile>

    <importColumns class="list">
        
        <column-descriptor>
            <columnIndex>1</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>name</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select b from ProductTypeEntity b where b.name = ?1</lookupQuery>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>1</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>description</name>
        </column-descriptor>

    </importColumns>
    <importFolder></importFolder>
</import-descriptor>