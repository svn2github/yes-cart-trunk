<import-descriptor>

    <entityIntface>com.npa.db.entity.SkuWarehouse</entityIntface>


    <importFile class="file-descriptor">
        <columnDelimeter>;</columnDelimeter>
        <textQualifier>&quot;</textQualifier>
        <ignoreFirstLine>true</ignoreFirstLine>
        <fileNameMask>product_entity*.csv$</fileNameMask>
        <fileEncoding>UTF-8</fileEncoding>
    </importFile>

    <importColumns class="list">

        <column-descriptor>
            <columnIndex>7</columnIndex>
            <fieldType>FIELD</fieldType>
            <!--<name>sku</name>-->
            <regExp>(.*)</regExp>
            <lookupQuery>select p from SkuWarehouseEntity p where p.sku.code = ?1 and p.warehouse.code = 'Основной'</lookupQuery>
        </column-descriptor>

        <column-descriptor>
            <columnIndex>14</columnIndex>
            <fieldType>FK_FIELD</fieldType>
            <name>sku</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select p from ProductSkuEntity p where p.code = ?1</lookupQuery>
        </column-descriptor>


        <column-descriptor>
            <columnIndex>-1</columnIndex>
            <fieldType>FK_FIELD</fieldType>
            <name>warehouse</name>
            <lookupQuery>select a from WarehouseEntity a where a.code = 'Основной'</lookupQuery>
        </column-descriptor>


        <column-descriptor>
            <columnIndex>12</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>quantity</name>
        </column-descriptor>


    </importColumns>
    <importFolder>src/test/resources/import</importFolder>
</import-descriptor>