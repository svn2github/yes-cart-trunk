<import-descriptor>

    <entityIntface>com.npa.db.entity.Availability</entityIntface>

    <importFile class="file-descriptor">
        <columnDelimeter>;</columnDelimeter>
        <textQualifier>&quot;</textQualifier>
        <ignoreFirstLine>true</ignoreFirstLine>
        <fileNameMask>^availability.csv$</fileNameMask>
        <fileEncoding>UTF-8</fileEncoding>
    </importFile>

    <importColumns class="list">

        <column-descriptor>
            <columnIndex>0</columnIndex>
            <fieldType>FIELD</fieldType>
            <name>name</name>
            <regExp>(.*)</regExp>
            <lookupQuery>select c from AvailabilityEntity c where c.name = ?1</lookupQuery>
        </column-descriptor>


    </importColumns>
    <importFolder>src/test/resources/import</importFolder>
</import-descriptor>